@model RegisterModel
@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/_VisitorLayout.cshtml";
}


<h2>Register</h2>

<form method="post" asp-controller="Admin" asp-action="Create">

    <div class="form-group">
        <label asp-for="Company.Name">Company name</label>
        <input asp-for="Company.Name" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Company.Code"> company code</label>
        <input type="number" asp-for="Company.Code" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Company.Logo"></label>
        <input asp-for="Company.Logo" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="UserName"></label>
        <input asp-for="UserName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Passowrd"></label>
        <input type="password" asp-for="Passowrd" class="form-control" />
    </div>

    <button class="btn btn-primary" type="submit" asp-action="Register">Register</button>
</form>